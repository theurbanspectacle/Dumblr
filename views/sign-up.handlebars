<div class="mdc-card login-wrapping">
  <h4 class="mdc-typography--headline4">Sign up</h4>
  <label class="mdc-text-field mdc-text-field--outlined" id="signup-username">
    <span class="mdc-notched-outline">
      <span class="mdc-notched-outline__leading"></span>
      <span class="mdc-notched-outline__notch">
        <span class="mdc-floating-label" id="signup-label-username">Username</span>
      </span>
      <span class="mdc-notched-outline__trailing"></span>
    </span>
    <input type="text" class="mdc-text-field__input" onkeyup="signupEnter(event)" aria-labelledby="signup-label-username">
  </label>
  <label class="mdc-text-field mdc-text-field--outlined" id="signup-password">
    <span class="mdc-notched-outline">
      <span class="mdc-notched-outline__leading"></span>
      <span class="mdc-notched-outline__notch">
        <span class="mdc-floating-label" id="signup-label-password">Password</span>
      </span>
      <span class="mdc-notched-outline__trailing"></span>
    </span>
    <input type="password" class="mdc-text-field__input" onkeyup="signupEnter(event)" aria-labelledby="signup-label-password">
  </label>

  <button class="mdc-button mdc-button--raised" onclick="signupAction()">
    <span class="mdc-button__label">Sign up</span>
  </button>
</div>

<script>
  mdc.textField.MDCTextField.attachTo(document.querySelector('#signup-username'));
  mdc.textField.MDCTextField.attachTo(document.querySelector('#signup-password'));
</script>